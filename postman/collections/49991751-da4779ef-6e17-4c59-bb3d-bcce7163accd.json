							"});",
							"pm.test(\"Response has Content-Type header\", function () {",
							"    pm.response.to.have.header(\"Content-Type\");",
							"});",
							"try {",
							"    var jsonData = pm.response.json();",
							"    pm.test(\"Response is a valid JSON object or array\", function () {",
							"        pm.expect([\"object\", \"array\"]).to.include(Array.isArray(jsonData) ? \"array\" : typeof jsonData);",
							"    });",
							"    pm.test(\"Response body is not empty\", function () {",
							"        if (Array.isArray(jsonData)) {",
							"            pm.expect(jsonData.length).to.be.above(0);",
							"        } else {",
							"            pm.expect(Object.keys(jsonData).length).to.be.above(0);",
							"        }",
							"    });",
							"} catch (e) {",
							"    pm.test(\"Response is not JSON\", function () {",
							"        pm.expect.fail(\"Response is not valid JSON\");",
							"    });",
							"}"
