								"type": "text/javascript",
								"exec": [
									"// Tests for 'Read Root' request",
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"pm.test(\"Response has Content-Type header\", function () {",
									"    pm.response.to.have.header(\"Content-Type\");",
									"});",
									"try {",
									"    var jsonData = pm.response.json();",
									"    pm.test(\"Response is a valid JSON object or string\", function () {",
									"        pm.expect([\"object\", \"string\"]).to.include(typeof jsonData);",
									"    });",
									"    pm.test(\"Response contains greeting or welcome message\", function () {",
									"        if (typeof jsonData === \"object\") {",
									"            var values = Object.values(jsonData).join(\" \").toLowerCase();",
									"            pm.expect(values).to.include(\"bienvenida\");",
									"        } else if (typeof jsonData === \"string\") {",
									"            pm.expect(jsonData.toLowerCase()).to.include(\"bienvenida\");",
									"        }",
									"    });",
									"} catch (e) {",
									"    pm.test(\"Response is not JSON\", function () {",
									"        pm.expect.fail(\"Response is not valid JSON\");",
									"    });",
									"}"
								]
